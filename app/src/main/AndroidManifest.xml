<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nerdoftheherd.tasker.rsync">

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RsyncForTasker">

        <activity
            android:name=".RsyncConfigActivity"
            android:exported="true"
            android:icon="@mipmap/ic_launcher"
            android:label="rsync">
            <intent-filter>
                <action android:name="com.twofortyfouram.locale.intent.action.EDIT_SETTING" />
            </intent-filter>
        </activity>

        <activity
            android:name=".DbclientConfigActivity"
            android:exported="true"
            android:icon="@mipmap/ic_launcher"
            android:label="dbclient">
            <intent-filter>
                <action android:name="com.twofortyfouram.locale.intent.action.EDIT_SETTING" />
            </intent-filter>
        </activity>

        <activity
            android:name=".PrivateKeyConfigActivity"
            android:exported="true"
            android:icon="@mipmap/ic_launcher"
            android:label="Generate Private Key">
            <intent-filter>
                <action android:name="com.twofortyfouram.locale.intent.action.EDIT_SETTING" />
            </intent-filter>
        </activity>
    </application>
</manifest>
